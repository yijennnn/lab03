void setup() {
  Serial.begin(9600);
}
float sensorValue = 0,sensorVoltage,R1; 
void loop() 
{
  for(int i=0;i<=5;i++)
  {
     sensorValue=(0.7*sensorValue)+(0.3*analogRead(A0));
    }
    sensorVoltage = sensorValue*2.5/4095;
    R1 = sensorVoltage*1030200/2.5;
    Serial.println("R1");
    Serial.println(R1);
    Serial.println("sensorVoltage");
    Serial.println(sensorVoltage);
    Serial.println("sensorValue");
    Serial.println(sensorValue);
    delay(3000);
}
